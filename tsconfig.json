{
//  break compile process
//  "extends": "@tsconfig/node16",
  "compilerOptions": {
    "lib": [
      "dom",
      "es2022"
    ],
    "module": "es2022",
    "esModuleInterop": true,
    "target": "es2022",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist",
    "baseUrl": ".",
    "strict": true,
    "stripInternal": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
